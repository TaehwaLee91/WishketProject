                    <script>
                        // Task, Term의 location box, open, close btn 등 script단
                        var $base = $('#work-location'),
                            $local = $('#location-selected-box'),
                            $close_btn = $('#close-btn'),
                            $open_btn = $('#location-select-btn'),
                            $sido_local  = $('#sido_local li'),
                            $sigun_local = $('#sigungu_local');

                        LocationReadyFunction($base, $local, $close_btn, $open_btn);
                        LocalBaseFunction($base, $sido_local, $sigun_local);

                        function LocationReadyFunction($base, $local, $close_btn, $open_btn) {
                            // Term, Task별 분리를 위한 Ready Main Function
                            $local.on('click', '.selected .remove', DeleteLocation);
                            $open_btn.on('click', function (e) {
                                $('.default_msg').addClass('active')
                                $('.error-text').css('display', 'none')
                                $('.send-matching-data').removeClass('error-active')
                                e.stopPropagation();
                                $base.css('display', 'inline-block');
                                if (!$local.hasClass('active')) {
                                    $local.addClass('active')
                                }
                                var $sido_local_input = $(this).closest('#condition-content-box').find('#sido_local input'),
                                    $sido_local = $(this).closest('#condition-content-box').find('#sido_local li');
                                $sido_local_input.trigger('change');
                                $sido_local.trigger('change');
                            });

                            $close_btn.on('click', function (e) {
                                e.stopPropagation();
                                $('.default_msg').removeClass('active')
                                $base.css('display', 'none');
                                if ($local.hasClass('active')) {
                                    $local.removeClass('active')
                                }
                                InputLocation($base.find('#sido_local input'), $local)
                            });
                            $('.location_selected_box').on('click', function (e) {
                                e.stopPropagation();
                                if($('#work-location').is(':visible') == true) {
                                    e.stopPropagation();
                                    $('#close-btn').click()
                                } else {
                                    $('#location-select-btn').click()
                                }
                            })
                        }

                        function LocationSelectedIsEmpty(){
                            // 현재 location_box에 선택된 지역구가 있는지 확인, 없으면 기본 메세지 show하는 함수
                            var $local = $('#selected-section').find('.location_selected_box')
                            var $default_msg = $local.children('.default_msg');
                            if ($local.children('.selected').length < 1){
                                $default_msg.css('display', 'inline-block')
                            } else{
                                $default_msg.hide()
                            }
                        }

                        function InputLocation($sido_input, $local){
                            // modal에서 Checked된 값들 가져와서 location_box에 넣는 함수
                            $local.children('span').remove();
                            $sido_input.each(function () {
                                var location_data = $(this).val().split('|');
                                for(var i=0; i<location_data.length - 1; i++){
                                    if (location_data[i] !== 'empty' || location_data[i] !== ''){
                                        var input_data = '<span class="body-3 selected">' + location_data[i] +
                                            '<span class="remove">' +
                                            '<img class="chip-btn-delete-normal" alt="chip-btn-delete-normal"' +
                                            "src='/img/mywishket/chip-btn-delete-normal.png'" + '>' +
                                            '</span></div>';
                                        $local.append(input_data)
                                    }
                                }
                            });
                            LocationSelectedIsEmpty()
                        }

                        function DeleteLocation(e){
                            // X가 클릭되었을 경우 해당 location 삭제 함수
                            var remove_val = $(this).parent().text().replace('X', '');

                            FindDeleteValue($('#sido_local input'), remove_val);
                            $(this).parent().remove();
                            LocationSelectedIsEmpty()
                            e.stopPropagation()
                        }

                        function FindDeleteValue($sido_local_active, remove_val){
                            // X가 클릭되고, Sido에 input valued되어있는 값 찾고 삭제하는 함수
                            $sido_local_active.each(function () {
                                if ($(this).val().indexOf(remove_val) !== -1){
                                    $(this).val(function (i, value){
                                        return value.replace(remove_val + '|', '');
                                    })
                                    return false
                                }
                            });
                        }

                        function LocalBaseFunction($base, $sido_local, $sigun_local){
                            $sigun_local.on('click', 'li input', function (e) {
                                // li가 아닌 checkbox를 클릭했을 떄 발생하는 문제 수정
                                e.stopPropagation();
                                if ($(this).is(':checked')){
                                    $(this).prop('checked', false)}
                                else{
                                    $(this).prop('checked', true)
                                }
                            })

                            $sido_local.on('click', SidoLocalClickedEvent);
                            $sigun_local.on('click','li', function(e){
                                e.stopPropagation();
                                var $checkbox = $(this).children('input[type="checkbox"]');
                                Checked($(this).children($checkbox));

                                LocationValueChecker($checkbox)
                            });


                            // 시도 값 변경이 감지된 경우, 시도 input에 일치된 값 찾아서 checked 처리해주기
                            $sido_local.on('change', function () {
                                var $local_input = $(this).closest('#condition-content-box').find('#sigungu_local input');
                                var $sido_active = $(this).closest('#condition-content-box').find('#sido_local .active');
                                var ignored = ($sido_active.text()).replace($sido_active.children('label').text(), '');

                                var $inserted_val = $sido_active.prev('input[type="hidden"]').val();
                                var allChecked = true
                                $local_input.each(function () {
                                    var this_val_text = ignored + ' > ' + $(this).val() + '|'
                                    if ($inserted_val.indexOf(this_val_text) !== -1){
                                        $(this).prop('checked', true)
                                    } else{
                                        if ($(this).val() === 'on'){
                                        } else {
                                            allChecked = false
                                            $(this).prop('checked', false)
                                        }
                                    }
                                })
                                $('.click_all').children('input').prop('checked', allChecked)
                            });

                            // sido input value값이 변경 감지된 경우, split 후 우측에 카운트
                            $base.find('#sido_local input').on('change', function () {
                                var list_ = $(this).val().split('|')
                                var $label_ = $(this).next().children('label')
                                if (list_.length -1 === 0){
                                    $label_.html('')
                                } else {
                                    $label_.html("(" + (list_.length - 1).toString() + ")");
                                }
                            })

                            $sigun_local.on('click','li.click_all', SigunguAllClickEvent);

                            // reset눌렀을 때, 카운트 삭제, checked False로 둔다.
                            $base.find('.reset_btn').on('click', function (e) {
                                e.stopPropagation();
                                $sido_local.children('label').each(function () {
                                    $(this).text('')
                                });
                                $sigun_local.find('li input[type="checkbox"]').each(function () {
                                    if ($(this).is(':checked')){
                                        $(this).prop('checked', false)
                                    }
                                })
                                var $sido_local_active = $('#work-location').find('#sido_local input[type="hidden"]');
                                $sido_local_active.each(function () {
                                    $(this).val('')
                                })
                            });

                        }

                        function SigunguAllClickEvent(e){
                            // xx시 전체를 클릭했을 때 나타나는 이벤트 함수
                            e.stopPropagation();
                            var is_checked = $(this).children('input[type="checkbox"]').is(':checked');
                            var $children = $(this).siblings('li').children('input[type="checkbox"]');
                            AllCheckTrigger(is_checked, $children);

                            if (is_checked){
                                $children.each (function () {
                                    AddValue($(this))
                                });

                            } else{
                                $children.each (function () {
                                    RemoveValue($(this))
                                });

                            }

                        }
                        function SidoLocalClickedEvent(e) {
                            // 시도를 클릭했을 때 나타나야하는 시군구를 가져오고 배치하는 함수
                            e.stopPropagation();
                            $(this).parent().children('li').removeClass('active');
                            var $this_trigger = $(this);
                            $this_trigger.addClass('active');
                            var value = $(this).val();
                            var $sigun = $(this).closest('.sido').siblings('.sigun').children('#sigungu_local');
                            $sigun.children().remove();
                            var url = "/address/sigungu/s/" + value + "/";
                            var insert_data = '';
                            var ignored = ($(this).text()).replace($(this).children('label').text(), '');

                            if (ignored === '해외'){
                                $.getJSON(url, function (j) {
                                    for (var i = 0; i < j.length; i++) {
                                        insert_data += '<li><input style="display: none" type="checkbox" value="' + j[i].fields.name + '">' +
                                            '<span class="matching-checkbox"></span>' +
                                            '<label class="body-2">' + j[i].fields.name + ' 전체' + '</label></li>';
                                    }
                                    $sigun.append(insert_data);

                                }).then(function () {
                                    $this_trigger.trigger('change');
                                })

                            }else {
                                insert_data = '<li class="click_all"><input style="display: none" type="checkbox">' +
                                    '<span class="matching-checkbox"></span>' +
                                    '<label class="body-2">' + ignored + ' 전체</label></li>';
                                $.getJSON(url, function (j) {
                                    for (var i = 0; i < j.length; i++) {
                                        insert_data += '<li><input style="display: none" type="checkbox" value="' + j[i].fields.name + '">' +
                                            '<span class="matching-checkbox"></span>' +
                                            '<label class="body-2">' + j[i].fields.name + '</label></li>';
                                    }
                                    $sigun.append(insert_data);

                                }).then(function () {
                                    $this_trigger.trigger('change');
                                })
                            }
                        }


                        function LocationValueChecker($checkbox){
                            // 시군구 li가 클릭되었을 때 checked 확인, value 넣거나 빼는 함수
                            if ($checkbox.is(':checked')){
                                AddValue($checkbox)
                            }

                            else{
                                RemoveValue($checkbox)
                            }
                        }

                        function AddValue($checkbox){
                            // 각 시도 input에 시군구 checkbox가 checked 된 경우 value 넣는 함수
                            var local_val = $checkbox.val();

                            if (local_val === 'on'){
                                return
                            }
                            var $sido_local_active = $('#work-location').find('#sido_local .active').prev('input[type="hidden"]');

                            // var $sido_local_active = $('#sido_local .active').prev('input[type="hidden"]');
                            $sido_local_active.val(function (i, value) {
                                var ignored = ($(this).next().text()).replace($(this).next().children('label').text(), '');
                                var this_val_text = ignored + ' > ' + local_val + '|';
                                if (value === 'empty'){
                                    return this_val_text
                                } else if (value.indexOf(this_val_text) !== -1){
                                    return $sido_local_active.val()
                                }

                                return value + this_val_text
                            }).trigger('change')
                        }

                        function RemoveValue($checkbox){
                            // 각 시도 input에 시군구 checkbox가 unchecked 된 경우 value 삭제하는 함수
                            var local_val = $checkbox.val(),
                                $sido_local_active = $('#work-location').find('#sido_local .active').prev('input[type="hidden"]');

                            $sido_local_active.val(function (i, value) {
                                var ignored = ($(this).next().text()).replace($(this).next().children('label').text(), '');
                                return value.replace(ignored + ' > ' + local_val + '|', '')
                            }).trigger('change')
                        }
